<%= render 'shared/navbar' %>

<nav class="navbar navbar-light">
  <a class="navbar-brand font-weight-bolder"></a>
  <form class="form-inline">
    <input class="form-control mr-sm-2" type="search" placeholder="State" aria-label="Search" name="state1">
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
  </form>
</nav>
<table class="table">
    <thead class="thead-light">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Start</th>
        <th>End</th>
        <th>State</th>
    </tr>
    </thead>
    <% @projects.each do |p| %>
        <tr>
            <td><%= p.name %> </td>
            <td><%= p.description %> </td>
            <td><%= p.start_date %> </td>
            <td><%= p.end_date %> </td>
            <td><%= p.state %> </td>
        </tr>
    <% end %>
</table>
